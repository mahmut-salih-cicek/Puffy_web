						android:id="@+id/linear6"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:padding="8dp"
						android:orientation="horizontal"/>
				</LinearLayout>
			</LinearLayout>
		</HorizontalScrollView>
	</LinearLayout>
	<LinearLayout
