<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="#263238"
		android:gravity="center_vertical"
		android:orientation="horizontal">
		<ImageView
			android:id="@+id/imageview3"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:src="@drawable/mennu"
			android:scaleType="center"/>
		<EditText
			android:id="@+id/edittext3"
			android:layout_width="170dp"
			android:layout_height="wrap_content"
			android:text="http://"
			android:textSize="12sp"
			android:textColor="#00BCD4"
			android:hint="Edit Text"
			android:textColorHint="#009688"
			android:singleLine="true"
			android:lines="1"
			android:imeOptions="actionSearch"/>
		<ImageView
			android:id="@+id/imageview4"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:src="@drawable/brow"
			android:scaleType="center"/>
		<ImageView
			android:id="@+id/imageview5"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:src="@drawable/loading"
			android:scaleType="center"/>
		<ImageView
			android:id="@+id/imageview20"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:src="@drawable/bookmarkk"
			android:scaleType="center"/>
		<ImageView
			android:id="@+id/imageview7"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:src="@drawable/fullscree"
			android:scaleType="center"/>
	</LinearLayout>
	<HorizontalScrollView
		android:id="@+id/hscroll2"
		android:layout_width="match_parent"
		android:layout_height="match_parent">
		<LinearLayout
			android:id="@+id/linear3"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="horizontal">
			<LinearLayout
				android:id="@+id/actionmenu1"
				android:layout_width="200dp"
				android:layout_height="match_parent"
				android:padding="8dp"
				android:background="#263238"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/linear21"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imegeview13"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:src="@drawable/book"
						android:scaleType="center"/>
					<TextView
						android:id="@+id/textview5"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingTop="8dp"
						android:paddingRight="25dp"
						android:paddingBottom="8dp"
						android:text="Evim"
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#03A9F4"
						android:lines="1"/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear26"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:orientation="vertical">
					<Button
						android:id="@+id/sethome"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="2dp"
						android:background="#0D47A1"
						android:text="Eve Ayarla"
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#03A9F4"/>
					<LinearLayout
						android:id="@+id/linear28"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:orientation="vertical"/>
					<Button
						android:id="@+id/loadhome"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="2dp"
						android:background="#0D47A1"
						android:text="Evi Yükle"
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#03A9F4"/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear22"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview14"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:src="@drawable/bookmark"
						android:scaleType="center"/>
					<TextView
						android:id="@+id/textview4"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Kitaplığım"
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#03A9F4"
						android:lines="1"/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear23"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview15"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:src="@drawable/histor"
						android:scaleType="center"/>
					<TextView
						android:id="@+id/textview1"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Geçmişim"
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#03A9F4"
						android:lines="1"/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear27"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview16"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:src="@drawable/neww"
						android:scaleType="center"/>
					<TextView
						android:id="@+id/textview2"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Yenilikler"
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#03A9F4"
						android:lines="1"/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear25"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview17"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:src="@drawable/aboutme"
						android:scaleType="center"/>
					<TextView
						android:id="@+id/textview3"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Hakkımızda"
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#03A9F4"
						android:lines="1"/>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/acm2"
				android:layout_width="10dp"
				android:layout_height="match_parent"
				android:padding="8dp"
				android:background="#263238"
				android:orientation="vertical"/>
			<LinearLayout
				android:id="@+id/acm3"
				android:layout_width="10dp"
				android:layout_height="match_parent"
				android:padding="8dp"
				android:background="#263238"
				android:orientation="vertical"/>
			<LinearLayout
				android:id="@+id/acm4"
				android:layout_width="10dp"
				android:layout_height="match_parent"
				android:padding="8dp"
				android:background="#263238"
				android:orientation="vertical"/>
			<LinearLayout
				android:id="@+id/acm5"
				android:layout_width="10dp"
				android:layout_height="match_parent"
				android:padding="8dp"
				android:background="#263238"
				android:orientation="vertical"/>
			<LinearLayout
				android:id="@+id/linear6"
				android:layout_width="10dp"
				android:layout_height="match_parent"
				android:padding="8dp"
				android:background="#263238"
				android:orientation="vertical"/>
			<LinearLayout
				android:id="@+id/acm7"
				android:layout_width="10dp"
				android:layout_height="match_parent"
				android:padding="8dp"
				android:background="#263238"
				android:orientation="vertical"/>
			<LinearLayout
				android:id="@+id/linear12"
				android:layout_width="360dp"
				android:layout_height="match_parent"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/menulimer"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:background="#263238"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview8"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:src="@drawable/bacckk"
						android:scaleType="center"/>
					<LinearLayout
						android:id="@+id/linear15"
						android:layout_width="20dp"
						android:layout_height="match_parent"
						android:padding="8dp"
						android:orientation="vertical"/>
					<ImageView
						android:id="@+id/imageview9"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:src="@drawable/neexxt"
						android:scaleType="center"/>
					<LinearLayout
						android:id="@+id/linear16"
						android:layout_width="20dp"
						android:layout_height="match_parent"
						android:padding="8dp"
						android:orientation="vertical"/>
					<ImageView
						android:id="@+id/imageview10"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:src="@drawable/mennuu"
						android:scaleType="center"/>
					<LinearLayout
						android:id="@+id/linear17"
						android:layout_width="20dp"
						android:layout_height="match_parent"
						android:padding="8dp"
						android:orientation="vertical"/>
					<ImageView
						android:id="@+id/imageview11"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:src="@drawable/zinoo"
						android:scaleType="center"/>
					<LinearLayout
						android:id="@+id/linear18"
						android:layout_width="20dp"
						android:layout_height="match_parent"
						android:padding="8dp"
						android:orientation="vertical"/>
					<ImageView
						android:id="@+id/imageview12"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:src="@drawable/exiit"
						android:scaleType="center"/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear19"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:background="#263238"
					android:gravity="center_horizontal"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/imageview18"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:src="@drawable/zoomin"
						android:scaleType="center"/>
					<LinearLayout
						android:id="@+id/linear20"
						android:layout_width="100dp"
						android:layout_height="match_parent"
						android:padding="8dp"
						android:orientation="vertical"/>
					<ImageView
						android:id="@+id/imageview19"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:src="@drawable/zoomin"
						android:scaleType="center"/>
				</LinearLayout>
				<ScrollView
					android:id="@+id/vscroll1"
					android:layout_width="match_parent"
					android:layout_height="match_parent">
					<WebView
						android:id="@+id/webview1"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"/>
				</ScrollView>
			</LinearLayout>
		</LinearLayout>
	</HorizontalScrollView>
</LinearLayout>
